datasource db {
    provider = "postgresql"
    url      = env("DATABASE_URL") // ← ここは実際に設定している環境変数名に合わせる
}

generator client {
    provider = "prisma-client-js"
}

model Todo {
    id        Int      @id @default(autoincrement())
    title     String
    completed Boolean  @default(false)
    createAt  DateTime @default(now())
}
